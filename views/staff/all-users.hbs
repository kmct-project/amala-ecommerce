<div class="page-content-wrapper main-content">
    <div class="container-md">
        <h4 class="mt-3 mb-3">Manage Your Users</h4>
        <div class="card">
            <div class="card-body">
                <table class="w-100" id="dataTable">
                    <thead>
                        <tr>
                            <th>No.</th>
                            <th data-type="date" data-format="YYYY/DD/MM">Joined Date</th>
                            <th>Email</th>
                            <th>Phone</th>
                            <th>Address</th>
                            <th>City</th>
                            <th>Manage</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each users}}
                        <tr>
                            <td>{{incremented @index}}</td>
                            <td>{{this.date}}</td>
                            <td>{{this.Email}}</td>
                            <td>{{this.Phone}}</td>
                            <td>{{this.Address}}</td>
                            <td>{{this.City}}</td>
                            <td><a href="" class="btn btn-danger btn-sm"><i class="bi bi-trash"></i></a></td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>